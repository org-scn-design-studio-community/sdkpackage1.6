
<component id="Tiles" 
	title="Fiori Tiles"
	icon="res/Tiles/Tiles.png"
	handlerType="sapui5" 
	modes="m"
	propertySheetPath="aps/org.scn.community.PropertyPage.div.html"
	group="ScnCommunityMBasics">

	<requireJs modes="m">res/Tiles/Tiles</requireJs>
  	<property id="enabled" type="boolean" title="Enabled"/>
  	<property id="iconFirst" type="boolean" title="Icon First"/>
  	<property id="buttonType" type="String" title="Button Type"/>
  	<property type="String" title="Text" id="text" group="Display"/>
  	<property type="String" title="Icon" id="icon" group="Display"/>
  	<property type="String" title="Active Icon" id="activeIcon" group="Display"/>
  	<property type="ScriptText" title="On Press" id="onpress" group="Events"/> 
 	<initialization>
 		<defaultValue property="text">Button</defaultValue>
 		<defaultValue property="iconFirst">true</defaultValue>
 		<defaultValue property="ButtonType">Default</defaultValue>
 		<defaultValue property="enabled">true</defaultValue>
		<defaultValue property="TOP_MARGIN">0</defaultValue>
		<defaultValue property="LEFT_MARGIN">0</defaultValue>
		<defaultValue property="RIGHT_MARGIN">auto</defaultValue>
		<defaultValue property="BOTTOM_MARGIN">auto</defaultValue>
 		<defaultValue property="WIDTH">400</defaultValue>
 		<defaultValue property="HEIGHT">48</defaultValue>
 	</initialization>
 </component>